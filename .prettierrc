{
    "printWidth": 80,
    "tabWidth": 4,
	"useTabs": false,
    "singleQuote": false,
    "semi": true,
    "bracketSpacing": true,
    "bracketSameLine": false,
	"trailingComma": "es5",
    "arrowParens": "always",
    "parser": "typescript",
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}